model {
  # Likelihood
  for (i in 1:N) {
    Y[i] ~ dnorm(mu[i], tau)T(0,)
    mu.temp[i] <- -n.qd*(T[i]-T0)*(T[i]-Tm)*((T[i]>T0))*((T[i]<Tm))
    mu[i]<- 1*(mu.temp[i]>1) + mu.temp[i]*(mu.temp[i]<1)*(mu.temp[i]>0) + 0*(mu.temp[i]<0)
  }
  # Prior
  qd<- -n.qd
  T0  ~ dgamma(hypers[1,1], hypers[2,1])
  Tm ~ dgamma(hypers[1,2], hypers[2,2])
  n.qd  ~ dgamma(hypers[1,5], hypers[2,5])
  sigma<-1/tau
  tau ~ dgamma(hypers[1,4], hypers[2,4])

}
